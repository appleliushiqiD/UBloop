.searchBox{ height: 100rpx; width: 100%; background: #e7e9fd; position: sticky; top: 0; z-index: 99;display: flex; justify-content: center; align-items: center; }
.searchBox .searchCon{width: 95%; height: 60%; display: flex; align-items: center;background: white; border-radius: 40rpx; }
.searchBox .searchCon .tip{ font-size: 30rpx; color: var(--global-font-color-g); padding-left: 10rpx; align-items: center; }
.searchBox .searchCon .inputBox{ width: 75%; height: 100%; display: flex; padding-left: 10rpx; align-items: center; position: relative; color: var(--global-font-color-g); }
.searchBox .searchCon .inputBox .icon{ height: 30rpx; padding: 4rpx; }
.searchBox .searchCon .inputBox .searchInput{ font-size: 24rpx; padding-top: 6rpx; }
.searchBox .searchCon .inputBox .cleanIcon{ height: 30rpx; position: absolute; right: 0; bottom: 16rpx; }


.history{ height: 250rpx;  border-bottom: 1rpx solid var(--global-part-line); position: relative; }
.history .historyTip{ font-size: 24rpx; padding: 10rpx 0; color: var(--global-font-color-l); display: flex; justify-content: center; align-items: center; }
.history .historyTip .icon{ width: 24rpx; height: 24rpx; padding: 4rpx 5rpx 0; }
.history .historyCon{width: 750rpx; height: 150rpx; padding: 0 20rpx; box-sizing: border-box; color: var(--global-font-color-g); font-size: 26rpx; display: flex; flex-wrap: wrap; }
.history .hisContBox{ height: 32rpx; background: white; display: flex; align-items: center; margin: 10rpx 10rpx; padding: 0 16rpx; box-sizing: border-box; border-radius: 16rpx; }

.showSearch{ min-height: 300rpx; }
.showSearch .headTip{ font-size: 24rpx; padding: 10rpx 0; color: var(--global-font-color-l);text-align: center; }



